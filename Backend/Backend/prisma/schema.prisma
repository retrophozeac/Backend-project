generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


// schema.prisma

model Transaction {

  TransactionID   String @id
  CustomerName    String
  TransactionDate DateTime
  Amount          Float
  Status          String?
  InvoiceURL      String?
}

